{
  "name": "iserv/zeit-bridge",
  "description": "Integrates the Zeit library into Symfony",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "doctrine/dbal": "^2.6",
    "iserv/zeit": "^1.1",
    "symfony/form": "^4.4 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "IServ\\Bridge\\Zeit\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "cotor": {
      "friendsofphp/php-cs-fixer": "^3.8",
      "phpstan/phpstan": "^1.6",
      "phpunit/phpunit": "^9.5",
      "vimeo/psalm": "^4.22"
    }
  }
}
