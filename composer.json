{
  "name": "iserv/zeit-bridge",
  "description": "Integrates the Zeit library into Symfony",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "doctrine/dbal": "2.*",
    "iserv/zeit": "^1.0-RC3",
    "symfony/form": "4.4.*|5.*"
  },
  "autoload": {
    "psr-4": {
      "IServ\\Bridge\\Zeit\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.iserv.eu"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*|5."
    }
  }
}
