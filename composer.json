{
  "name": "iserv/zeit-bridge",
  "description": "Integrates the Zeit library into Symfony",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "doctrine/dbal": "^2.12 || ^3.0",
    "iserv/zeit": "^1.1",
    "symfony/form": "^5.4.26 || ^6.3"
  },
  "autoload": {
    "psr-4": {
      "IServ\\Bridge\\Zeit\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "cotor": {
      "friendsofphp/php-cs-fixer": "^3.26",
      "phpstan/phpstan": "^1.10",
      "phpunit/phpunit": "^9.5",
      "vimeo/psalm": "^5.10"
    }
  }
}
